{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["lib/*"],
      "@/lib/*": ["src/lib/*"],
      "@/components/*": ["src/components/*"],
      "@/types/*": ["src/types/*"],
      "~/*": ["src/*"]
    },
    "types": ["js-cookie", "@types/qs"]
  },
  "extends": "tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
